{
  "name": "twitch-extension-starter",
  "version": "1.2.0",
  "description": "This will get you started developing Twitch extensions using React!",
  "main": "src/index.js",
  "author": "Martin Beierling <mbeierling@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"parcel watch src/pages/config.html src/pages/video_component.html -d dist --public-url ./\" \"yarn dev-server\"",
    "dev-server": "node server.js",
    "build": "parcel build src/pages/config.html src/pages/video_component.html -d dist --no-minify --public-url ./",
    "zip": "cross-env bestzip dist_zip/$npm_package_version.zip dist/*",
    "package": "rm -rf dist && npm run build && npm run zip"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "node-fetch": "^2.3.0",
    "react": "^16.2.0",
    "react-bootstrap": "^1.0.0-beta.3",
    "react-dom": "^16.2.0",
    "react-toastify": "^4.5.2",
    "redux": "^4.0.1",
    "vague-time": "^2.4.2"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "^6.24.1",
    "bestzip": "^1.1.4",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "eslint": "4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "2.34.1",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "^4.16.2",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "parcel-bundler": "^1.8.1",
    "path": "^0.12.7"
  }
}
